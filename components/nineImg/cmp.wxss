/* components/nineImg/cmp.wxss */

.container{
    width:700rpx;
}
.img-container{
    position: relative;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    width:700rpx;
    height: 700rpx;
    overflow: hidden;
}
.img-container image{
    width:31%;
    height: 31%
}
.img-container .img-num{
    position: absolute;
    right: 80rpx;
    bottom:80rpx;
    background-color: rgba(0,0,0,.7);
    padding:5rpx 10rpx;
    color: #fff;
    font-size: 24rpx;
}
.text-container{
    padding:0 30rpx;
    font-size: 40rpx;
    font-weight: 400;
    letter-spacing: 2rpx;
}